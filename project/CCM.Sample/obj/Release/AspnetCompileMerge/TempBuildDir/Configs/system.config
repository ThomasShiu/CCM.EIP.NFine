<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- ================== 1：開發系統相關配置 ================== -->
  <!-- 登陸提供者模式：Session、Cookie-->
  <add key="LoginProvider" value="Cookie" />
  <!-- 啟用系統日誌-->
  <add key="IsLog" value="true" />
  <!-- 資料庫超時間-->
  <add key="CommandTimeout" value="180" />
  <!--啟用IP過濾 -->
  <add key="IsIPFilter" value="false" />
  <!-- ================== 2：系統軟體參數配置 ================== -->
  <!-- 聯繫我們 -->
  <add key="Contact" value="www.ccm3s.co," />
  <!-- 軟體名稱 -->
  <add key="SoftName" value="CCM快速開發框架" />
  <!-- 軟體版本 -->
  <add key="Version" value="1.0" />
  <!-- 軟體授權碼 測試備用 -->
  <add key="LicenceKey" value="83eda07f-ead3-4050-bcee-4f41e57a7e16" />
  <!-- ================== 3：外部郵件參數 ================== -->
  <!-- 設置郵箱名稱 -->
  <add key="MailName" value="郵件中心" />
  <!-- 設置郵箱位址 -->
  <add key="MailUserName" value="b050502@ccm3s.com" />
  <!-- 設置郵箱密碼 -->
  <add key="MailPassword" value="123456" />
  <!-- 設置郵箱主機 -->
  <add key="MailHost" value="smtp.ym.163.com" />
</appSettings>
